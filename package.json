  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npm run extract-content-routes && ng serve",
    "ng": "ng",
    "start": "npm run dev",
    "build": "npm run extract-content-routes && ng build",
    "watch": "npm run extract-content-routes && ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "extract-content-routes": "find src/content -type f -name '*.md' -exec sh -c 'echo \"/$(dirname {} | cut -d/ -f3)/$(basename -s .md {})\"' \\; | jq -R -s -c 'split(\"\n\")[:-1]' > content-routes.json && npx prettier -w content-routes.json"
  },
